{
    "name": "bugrabozkurtt/inter-service-communication",
    "description": "Inter-service communication package for Laravel 11 projects",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.7"
    },
    "autoload": {
        "psr-4": {
            "BugraBozkurt\\InterServiceCommunication\\": "src/"
        },
        "files": [
            "src/Helpers/Tools.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BugraBozkurt\\InterServiceCommunication\\Providers\\InterServiceCommunicationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
